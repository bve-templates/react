{
  "name": "{{ name }}",
  "version": "1.0.0",
  "description": "{{ description }}",
  "main": "src/index.js",
  "scripts": {
    "prestart": "npm run clean{{#git}} & npm run clone{{/git}}",
    "start": "npm run build:dll && cross-env NODE_ENV=development webpack-dev-server",
    "prebuild": "npm run clean{{#git}} & npm run clone{{/git}}",
    "build": "npm run build:dll && cross-env NODE_ENV=production webpack",
    {{#cdn}}
    "postbuild": "npm run cdn",
    {{/cdn}}
    "build:dll": "cross-env NODE_ENV=production webpack --config webpack.dll.config.js",
    {{#git}}
    "clone": "bv-git clone",
    {{/git}}
    {{#cdn}}
    "cdn": "bv-cdn upload",
    {{/cdn}}
    "clean": "rimraf dist"{{#lint}},
    "eslint": "eslint -f node_modules/eslint-friendly-formatter --fix \"./src/**/*.{js,jsx}\"",
    "stylelint": "stylelint --fix \"./src/**/*.{css,scss,less}\"",
    "lint": "npm run eslint && npm run stylelint",
    "lint-staged": "lint-staged"{{/lint}}
  },
  {{#lint}}
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  {{/lint}}
  "author": "{{ author }}",
  "license": "ISC",
  "dependencies": {
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-router-dom": "^5.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.4.4",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/plugin-proposal-optional-chaining": "^7.2.0",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.4.4",
    "@babel/preset-env": "^7.4.4",
    "@babel/preset-react": "^7.0.0",
    "@babel/runtime": "^7.4.4",
    "autoprefixer": "^9.5.1",
    "babel-loader": "^8.0.5",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-preset-minify": "^0.5.0",
    {{#cdn}}
    "bv-cdn-cli": "^0.0.3",
    {{/cdn}}
    {{#git}}
    "bv-git-cli": "^0.0.8",
    {{/git}}
    "core-js": "^3.0.1",
    "cross-env": "^5.2.0",
    "css-loader": "^2.1.1",
    "cssnano": "^4.1.10",
    {{#lint}}
    "eslint": "^5.16.0",
    "eslint-config-bv": "^0.0.5",
    "eslint-friendly-formatter": "^4.0.1",
    {{/lint}}
    "file-loader": "^3.0.1",
    "html-loader": "^0.5.5",
    "html-webpack-include-assets-plugin": "^2.0.0",
    "html-webpack-plugin": "^3.2.0",
    {{#lint}}
    "husky": "^2.3.0",
    {{/lint}}
    "image-webpack-loader": "^4.6.0",
    {{#if_eq csscompiler 'Less'}}
    "less": "^3.9.0",
    "less-loader": "^5.0.0",
    {{/if_eq}}
    {{#lint}}
    "lint-staged": "^8.1.7",
    {{/lint}}
    "mini-css-extract-plugin": "^0.6.0",
    {{#if_eq csscompiler 'Sass'}}
    "node-sass": "^4.12.0",
    {{/if_eq}}
    "path": "^0.12.7",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^3.0.0",
    "react-hot-loader": "^4.8.4",
    "react-loadable": "^5.5.0",
    "rimraf": "^2.6.3",
    {{#if_eq csscompiler 'Sass'}}
    "sass-loader": "^7.1.0",
    {{/if_eq}}
    "style-loader": "^0.23.1",
    {{#lint}}
    "stylelint": "^10.0.1",
    "stylelint-config-bv": "^0.0.5",
    {{/lint}}
    "uglifyjs-webpack-plugin": "^2.1.2",
    "url-loader": "^1.1.2",
    "webpack": "^4.30.0",
    "webpack-bundle-analyzer": "^3.3.2",
    "webpack-cli": "^3.3.1",
    "webpack-dev-server": "^3.3.1",
    "webpack-manifest-plugin": "^2.0.4",
    "webpack-merge": "^4.2.1"
  }
}